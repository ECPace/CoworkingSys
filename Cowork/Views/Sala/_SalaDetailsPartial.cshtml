@model Cowork.Models.Sala

<h4>Detalhes da Sala</h4>
<dl class="row">
    <dt class="col-sm-2">Nome</dt>
    <dd class="col-sm-10">@Model.Nome</dd>

    <dt class="col-sm-2">Capacidade</dt>
    <dd class="col-sm-10">@Model.Capacidade</dd>

    <dt class="col-sm-2">Preço por Hora</dt>
    <dd class="col-sm-10">@Model.PrecoPorHora</dd>

    <dt class="col-sm-2">Latitude</dt>
    <dd class="col-sm-10">@Model.Latitude</dd>

    <dt class="col-sm-2">Longitude</dt>
    <dd class="col-sm-10">@Model.Longitude</dd>
</dl>
